
<span id="art_listing">
  <div class="row">
    <div class="span16">
      <h1>My Subscriptions:</h1>
      <% @current_user.followings.each do |user| %>
      <h3> <%= user.name || user.email %> <button id="unsubscribe" data-user-id="<%= user.id %>">unsubscribe</button></h3>
      <% end %>
    </div>
  </div>
  <div class="container">
    <div class="row">
      <div class="span1">
        &nbsp
      </div>
      <div class="span8">
        <%= render :partial => "art_listing", :collection => @followings_art %>
      </div>
    </div>
  </div>
</span>
